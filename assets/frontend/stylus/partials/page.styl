.flip-container
    position relative
//     width:50%
//     height: 100%;
//     margin:auto;
.flip-containerStyle2
    margin-top 30%		
.flip-container-front div
    width: 250px;
.flip-container-front
    text-align:center;
    position: absolute;
    backface-visibility: hidden;
    transition:  linear 0.7s;
    transform: rotateY(0deg);
    cursor: pointer
.flip-container-front:after
    content url(../images/svg/flip-btn.svg)
    position: absolute
    top 230px
    right 10px
    cursor: pointer
.flip-container-back
    position: absolute;
    backface-visibility: hidden;
    transition:  linear 0.7s;
    transform: rotateY(180deg);
    background-color: colorElem 
    height:300px;
    width: 250px;
    text-align: center;
    border-radius: 40px

.flip-container:hover > .flip-container-front
    transform: rotateY(-180deg);
    height: 300px;
    width: 250px;

.flip-container:hover > .flip-container-back
    transform: rotateY(0deg);
